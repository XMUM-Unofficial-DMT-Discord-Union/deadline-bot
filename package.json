{
  "main": "build/main.js",
  "type": "module",
  "scripts": {
    "build": "sudo npm run clean && tsc",
    "clean": "sudo rm -rf build/",
    "dev-update": "npm run build && node -r dotenv/config build/deploy_commands.js dotenv_config_path=./.env.development.local",
    "dev-start": "npm run build && node -r dotenv/config . dotenv_config_path=./.env.development.local",
    "update": "sudo npm run build && node -r dotenv/config build/deploy_commands.js dotenv_config_path=./.env.production.local",
    "start": "sudo npm run build && node -r dotenv/config . dotenv_config_path=./.env.production.local"
  },
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "@prisma/client": "^3.11.0",
    "@types/node": "*",
    "big.js": "^6.1.1",
    "dayjs": "^1.10.7",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.5.0",
    "dotenv": "^10.0.0",
    "firebase": "^9.6.3",
    "humanize-duration": "^3.27.1",
    "ms": "^3.0.0-canary.1",
    "node-schedule": "^2.1.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/big.js": "^6.1.2",
    "@types/humanize-duration": "^3.27.1",
    "@types/ms": "^0.7.31",
    "@types/node-schedule": "^1.3.2",
    "pm2": "^5.1.2",
    "prisma": "^3.11.1",
    "ts-node": "^10.4.0"
  },
  "prisma": {
    "seed": "ts-node --esm prisma/seed.ts"
  }
}
